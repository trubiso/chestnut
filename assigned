@extern func print_integer(anon x: uint64);
@extern func putchar(c: char);

func fibonacci(count: uint64) {
	const a = 0;
	mut b = 1;
	mut i = 0;
'loop:
	i = i + 1;
	print_integer(a);
	putchar(' ');
	const c = a + b;
	a = b;
	b = c;
	if (i < count) goto 'loop;
}

func skip_vardecl() {
	goto 'next;
'skip:
	const a: uint8;
'next:
	a = 3;
	const q = a;
}

struct Test { field: bool }

@extern
func main() {
	const a: uint8 = 3;
	a = 4;
	const b = undefined;
	b = 3;
	b = 4;
	const test = Test { field: true };
	test.field = false;
}
